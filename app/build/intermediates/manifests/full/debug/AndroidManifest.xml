        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="local.koki.android.eventory.viewController.HomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="local.koki.android.eventory.viewController.ConfigAtPlaceActivity" />
        <activity android:name="local.koki.android.eventory.viewController.ConfigAtJenreAcitivty" />
    </application>

</manifest>